{
    "name": "strandhus-october-theme",
    "version": "2.0.0-alpha.0",
    "description": "Strandhus OctoberCMS Theme",
    "main": "assets/ts/main.ts",
    "typings": "assets/ts/types",
    "scripts": {
        "webpack": "webpack --env production",
        "webpack:dev": "webpack --env development",
        "lint": "eslint . --ext .ts,.tsx --fix && tsc --noEmit",
        "test": "npm run lint",
        "build": "concurrently \"npm:build:prod:*\"",
        "build:prod:ts": "npm run lint && npm run webpack",
        "build:prod:scss": "sass --style=compressed --no-source-map assets/scss/theme.scss assets/css/theme.css",
        "build:dev": "concurrently \"npm:build:dev:*\"",
        "build:dev:ts": "npm run lint && npm run webpack:dev",
        "build:dev:scss": "sass assets/scss/theme.scss assets/css/theme.css",
        "watch": "concurrently \"npm:watch:*\"",
        "watch:ts": "npm run webpack:dev -- --watch",
        "packages:upgrade": "yarn upgrade-interactive"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ribajs/doc"
    },
    "keywords": [
        "Bootstrap",
        "Bootstrap4",
        "OctoberCMS",
        "Art+Code Studio",
        "Riba",
        "Rivets",
        "tinybind",
        "Hypnose",
        "hypnosis"
    ],
    "author": "Art+Code Studio <hi@artandcode.studio>",
    "license": "MIT",
